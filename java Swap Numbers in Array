package com.company;

import java.util.Arrays;

public class SwapNumber {
    public static void main(String[] args) {
        int[] arr = { 1, 2, 3, 4, 6};
        swap(arr,0,4);
        System.out.println(Arrays.toString(arr));  //[6, 2, 3, 4, 1]
    }

    static  void swap(int[] arr, int index1, int index2){
        int temp = arr[index1];
        arr[index1] = arr[index2];
        arr[index2] = temp;
    }
}
